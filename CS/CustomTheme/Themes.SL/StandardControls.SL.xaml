    <Style TargetType="ToolTip">
        <Setter Property="Foreground" Value="White" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToolTip">
                    <Grid HorizontalAlignment="Left" VerticalAlignment="Top">
                        <Border Background="#FF484848" CornerRadius="3" Padding="1" Opacity="0.9">
                            <Border Background="#FF6E6E6E" CornerRadius="2" />
                        </Border>
                        <ContentPresenter Margin="7,4,7,5" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

